<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Normal" Color="{DynamicResource {x:Static SystemColors.WindowTextColorKey}}" />
    <SolidColorBrush x:Key="Warning">Red</SolidColorBrush>
    <SolidColorBrush x:Key="Unfocused" Color="{DynamicResource {x:Static SystemColors.ControlDarkColorKey}}" />
</ResourceDictionary>